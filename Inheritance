public class Animal {
     void  eat(){
         System.out.println("Animal is eating");
     }
}

# Next Class start from here!

public class Dog extends Animal{
    void bark(){
        System.out.println("Dog is Barking...");
    }
}

# Next Class start from here!

public class Main {
    static void main(String[] args) {
        Dog d = new Dog();
        d.eat();
        d.bark();
    }
}
